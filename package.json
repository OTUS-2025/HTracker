    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.41.0",
    "prettier": "3.8.1",
    "supabase": "^2.76.8",
    "typescript": "~5.9.3",
