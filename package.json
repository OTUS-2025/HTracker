    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@primeuix/themes": "^2.0.3",
    "@primevue/forms": "^4.5.4",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "pinia": "^3.0.4",
    "primevue": "^4.5.4",
    "tailwindcss": "^4.1.18",
